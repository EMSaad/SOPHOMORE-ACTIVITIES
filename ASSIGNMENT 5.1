from random import randint 

score = 0

for i in range (1,11):
    a = randint(1,10)
    b = randint(1,10)
    answer = a*b
    print("Question", i, ":", a, "X", b)
    c = int(input("Answer:"))
    
    
    if c == answer:
        print("Correct!")
        score =+ 1
        
    else:
        print("Wrong, The right answer is: ", answer)
        
